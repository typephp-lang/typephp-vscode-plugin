{
    "name": "typephp",
    "displayName": "typephp",
    "publisher": "typephp",
    "description": "typephp support",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/typephp-lang/typephp-vscode-plugin"
      },
    "homepage": "https://github.com/typephp-lang/typephp-vscode-plugin/blob/main/README.md",

    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "typephp",
            "aliases": ["typephp", "typephp"],
            "extensions": [".tp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "typephp",
            "scopeName": "source.tp",
            "path": "./syntaxes/typephp.tmLanguage.json"
        }]
    }
}